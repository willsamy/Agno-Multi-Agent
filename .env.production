# Configurações de Produção para Vercel
# IMPORTANTE: Configure estas variáveis no painel da Vercel

# Configurações da API Google Gemini
GOOGLE_API_KEY=sua_chave_api_de_producao

# Configurações dos Modelos Gemini
RESEARCH_MODEL=gemini-2.5-flash-lite
CODER_MODEL=gemini-2.5-flash
SUPERVISOR_MODEL=gemini-2.5-flash

# Configurações do Servidor (Vercel gerencia automaticamente)
SERVER_HOST=0.0.0.0
SERVER_PORT=8000

# Configurações de Retry
MAX_RETRIES=3
BASE_DELAY=1
CODER_MAX_RETRIES=3
CODER_BASE_DELAY=2

# Configurações de Debug (desabilitado em produção)
DEBUG_MODE=false
VERBOSE_LOGS=false

# Configurações de API por nível (opcional - para múltiplas chaves)
API_BASE_1=sua_chave_base_1
API_BASE_2=sua_chave_base_2
API_MEDIO_1=sua_chave_medio_1
API_MEDIO_2=sua_chave_medio_2
API_AVANCADO_1=sua_chave_avancado_1
API_AVANCADO_2=sua_chave_avancado_2

# Modelos por nível
MODEL_BASE=gemini-2.5-flash-lite
MODEL_MEDIO=gemini-2.5-flash
MODEL_AVANCADO=gemini-2.5-pro