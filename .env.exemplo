# Configurações da API Google Gemini
GOOGLE_API_KEY=sua_chave_api_aqui

# Configurações dos Modelos Gemini
# Modelos disponíveis: gemini-1.5-flash, gemini-1.5-pro, gemini-2.0-flash-exp, gemini-2.5-flash, gemini-2.5-flash-lite
RESEARCH_MODEL=gemini-2.5-flash-lite
CODER_MODEL=gemini-2.5-flash
SUPERVISOR_MODEL=gemini-2.5-flash

# Configurações do Servidor
SERVER_HOST=0.0.0.0
SERVER_PORT=8192

# Configurações de Retry
MAX_RETRIES=3
BASE_DELAY=1
CODER_MAX_RETRIES=3
CODER_BASE_DELAY=2

# Configurações de Debug
DEBUG_MODE=false
VERBOSE_LOGS=true