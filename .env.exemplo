# Configurações da API Google Gemini
# Chaves de API para cada nível de modelo

# Modelo Base (60 req/min)
API_BASE_1=sua_chave_api_base_1
API_BASE_2=sua_chave_api_base_2

# Modelo Médio (30 req/min)  
API_MEDIO_1=sua_chave_api_medio_1
API_MEDIO_2=sua_chave_api_medio_2

# Modelo Avançado (10 req/min)
API_AVANCADO_1=sua_chave_api_avancado_1
API_AVANCADO_2=sua_chave_api_avancado_2

# Sistema de Modelos por Complexidade
MODEL_BASE=gemini-2.5-flash-lite
MODEL_MEDIO=gemini-2.5-flash
MODEL_AVANCADO=gemini-2.5-pro

# Configurações de Rate Limit
RATE_LIMIT_BASE=60
RATE_LIMIT_MEDIO=30
RATE_LIMIT_AVANCADO=10

# Configurações do Servidor
SERVER_HOST=0.0.0.0
SERVER_PORT=8192

# Configurações de Retry
MAX_RETRIES=3
BASE_DELAY=1
CODER_MAX_RETRIES=3
CODER_BASE_DELAY=2

# Configurações de Debug
DEBUG_MODE=false
VERBOSE_LOGS=true